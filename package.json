{
  "name": "pkg-website",
  "version": "0.0.1",
  "type": "module",
  "description": "Reduce a website to an executable file",
  "main": "dist/make.cjs",
  "bin": {
    "pkg-website": "dist/make.cjs"
  },
  "scripts": {
    "build": "node --experimental-specifier-resolution=node ./build.js",
    "pkg-src": "node --experimental-specifier-resolution=node ./src/make.js",
    "pkg-dist": "npm run build && node ./dist/make.cjs",
    "test:unit": "./node_modules/.bin/mocha --experimental-specifier-resolution=node ./src/**/*.test.js",
    "test": "npm run test:unit",
    "lint": "./node_modules/.bin/eslint --ext .js . --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "github.com/memob0x/pkg-website"
  },
  "keywords": [ 
    "browser",
    "pkg",
    "executable",
    "bundle",
    "build",
    "kiosk",
    "chrome",
    "firefox",
    "inject",
    "binary"
  ],
  "author": "memob0x",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.18.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "argv": "^0.0.2",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "pkg": "^5.7.0",
    "puppeteer": "^14.1.1",
    "rollup": "^2.74.1"
  }
}
